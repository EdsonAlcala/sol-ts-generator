{
  "name": "sol-ts-generator",
  "version": "1.1.3",
  "description": "Generate typescript type definitions from Ethereum smart contracts ABI that will allow you to write static typed Typescript tests in your Truffle projects",
  "types": "dist/index.d.ts",
  "bin": "dist/index.js",
  "scripts": {
    "test": "jest --watch",
    "build": "tsc",
    "test:index": "jest index.test.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EdsonAlcala/sol-ts-generator.git"
  },
  "keywords": [
    "solidity",
    "typescript"
  ],
  "author": "Edson Alcala <edson_alcala@msn.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/glob": "^5.0.35",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.4",
    "@types/prettier": "^1.13.2",
    "jest": "^23.4.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "command-line-args": "^5.0.2",
    "glob": "^7.1.2",
    "merge": "^1.2.1",
    "prettier": "^1.14.2"
  },
  "peerDependencies": {
    "merge": ">=1.2.1"
  },
  "jest": {
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
